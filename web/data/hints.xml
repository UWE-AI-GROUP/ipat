<?xml version="1.0"?><!--To change this license header, choose License Headers in Project Properties.To change this template file, choose Tools | Templatesand open the template in the editor.File InteractionDecriptions.xml@author Jim Smith @data 14th May 2015@brief attempt to describe all of the interactions that a system has with a user in xml format--><root>    <interaction>        <name>            globalScore        </name>        <displaytype>            range        </displaytype>        <displayText>            Score        </displayText>        <range-min>            0        </range-min>        <range-max>            10        </range-max>        <default>            5        </default>        <KernelAffected>         </KernelAffected>        <KernelVariableAffected>                    </KernelVariableAffected>        <ProfileVariablesAffected>            globalScore        </ProfileVariablesAffected>        <Effect>            setNewValue        </Effect>    </interaction>    <interaction>        <name>            FreezeBGColours        </name>        <displaytype>            checkbox        </displaytype>        <displayText>            Freeze Background        </displayText>        <default>            False        </default>        <KernelAffected>         </KernelAffected>        <KernelVariableAffected>                    </KernelVariableAffected>        <ProfileVariablesAffected>            Page_bg_Red        </ProfileVariablesAffected>        <ProfileVariablesAffected>            Page_bg_Blue        </ProfileVariablesAffected>        <ProfileVariablesAffected>            Page_bg_Green        </ProfileVariablesAffected>        <Effect>            setRateOfEvolutionEqualZero        </Effect>    </interaction>    <interaction>        <name>            ChangeFontSize        </name>        <displaytype>            range        </displaytype>        <displayText>            Text Size        </displayText>        <range-min>            0        </range-min>        <range-max>            10        </range-max>        <default>            5        </default>        <KernelAffected>             h1        </KernelAffected>        <KernelAffected>             h2        </KernelAffected>        <KernelAffected>             p        </KernelAffected>        <KernelVariableAffected>            fontsize        </KernelVariableAffected>        <ProfileVariablesAffected>                    </ProfileVariablesAffected>        <Effect>            moderateByNewValue        </Effect>    </interaction>   </root>